{
  "log": {
    "loglevel": "warning",
    "access": "D:\\log\\access.log",
    "error": "D:\\log\\error.log"
  },
  "dns": {
    "servers": [
      "localhost"
    ]
  },
  "inbounds": [
    {
      "listen": "0.0.0.0",
      "port": 1086,
      "protocol": "socks",
      "domainOverride": [
        "tls",
        "http"
      ],
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    },
    {
      "port": 1186,
      "listen": "0.0.0.0",
      "protocol": "http",
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "{DOMAIN}",
            "port": 443,
            "users": [
              {
                "id": "{UUID_PLACE_HOLDER}",
                "alterId": 233,
                "security": "auto"
              }
            ]
          }
        ]
      },
            "streamSettings": {
        "network": "ws",
        "security": "tls",
        "tlsSettings": {
          "serverName": "{DOMAIN}"
        },
        "wsSettings": {
          "path": "/",
          "headers": {
            "Host": "{DOMAIN}"
          }
        }
      },
      "mux": {
        "enabled": true
      }
    },
     {
      "protocol": "freedom",
      "settings": {},
      "tag": "direct"
    },
    {
      "protocol": "blackhole",
      "settings": {},
      "tag": "block"
    }
  ],
  "routing": {
    "domainStrategy": "IPOnDemand",
    "rules": [
      {
        "type": "field",
        "outboundTag": "block",
        "domain": [
          "ext:h2y.dat:ad"
        ]
      },
      {
        "type": "field",
        "outboundTag": "proxy",
        "domain": [
          "ext:h2y.dat:gfw"
        ]
      }
    ]
  }
}
